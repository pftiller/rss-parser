{
  "name": "rss-ingester",
  "version": "2.0.0",
  "description": "Serverless GCP Cloud Function to fetch episode attributes from APMG public feeds app.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node node_modules/@google-cloud/functions-framework --target=parseRss",
    "test": "node --inspect node_modules/.bin/functions-framework --target=parseRss",
    "deploy": "gcloud functions deploy getStreamguysEpisodeAttributes --trigger-topic getStreamguysEpisodeAttributes ---runtime=nodejs16 --entry-point=parseRss --service-account=apmg-data-warehouse@appspot.gserviceaccount.com"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/bigquery": "^6.2.0",
    "@google-cloud/functions-framework": "^3.2.0",
    "moment": "^2.29.4",
    "rss-parser": "^3.10.0"
  }
}
